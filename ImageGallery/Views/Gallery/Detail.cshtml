
@model ImageGallery.Models.GalleryDetailModel

<div class="container body-content">
    <div class="row" image-detail-conteiner>
        <div class="image-detail">

            <img src="@Model.Url" alt="@Model.Title" />
        </div>

    </div>
    <div class="row">
        <span>Title: @Model.Title</span>


    </div>
    <div class="row">
        <a class="btn btn-primary" asp-controller="Gallery" asp-action="Index">
            Back to Gallery
         </a>


    </div>

</div>
